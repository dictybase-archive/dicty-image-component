{
  "name": "dicty-image-component",
  "version": "11.0.0",
  "description": "React library with support for modern image formats",
  "license": "BSD-2-Clause",
  "main": "dist/index.js",
  "module": "dist/dicty-image-component.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dictyBase/dicty-image-component.git"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests --watch",
    "test-coverage": "tsdx test --passWithNoTests --coverage",
    "lint": "tsdx lint",
    "prepare": "tsdx build && husky install",
    "styleguide": "FAST_REFRESH=false styleguidist server",
    "styleguide:build": "FAST_REFRESH=false styleguidist build",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-image": "^2.1.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.1",
    "@semantic-release/github": "^7.2.3",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/react": "^17.0.33",
    "eslint-plugin-prettier": "^4.0.0",
    "file-loader": "^6.2.0",
    "husky": "7",
    "lint-staged": "^11.1.2",
    "node-notifier": "10.0.0",
    "prettier": "^2.4.1",
    "react": "^16.14.0",
    "react-docgen-typescript": "^2.1.0",
    "react-dom": "^16.14.0",
    "react-styleguidist": "^11.1.7",
    "semantic-release": "^17.4.7",
    "ts-loader": "~8.2.0",
    "tsdx": "^0.14.1",
    "webpack": "4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ],
    "collectCoverageFrom": [
      "src/components/*.{ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/dist/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ]
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "10 kB"
    }
  ]
}
